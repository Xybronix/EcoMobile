{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "image": "latest"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "image": "latest",
        "env": {
          "EXPO_IMAGE_OPTIMIZER": "true",
          "EXPO_OPTIMIZE_IMAGES": "true",
          "EXPO_PUBLIC_API_URL": "https://env-freebike-xybronix.hidora.com/api/v1"
        }
      },
      "ios": {
        "env": {
          "EXPO_PUBLIC_API_URL": "https://env-freebike-xybronix.hidora.com/api/v1"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}